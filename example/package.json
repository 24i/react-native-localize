{
  "name": "react-native-localize-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean-modules": "rm -rf ./node_modules/react-native-localize/{example,node_modules}",
    "clean": "rm -rf ./node_modules ./ios/Pods",
    "postinstall": "yarn clean-modules && yarn pod-install",
    "pod-install": "cd ./ios && pod install && cd ..",
    "start": "react-native start",
    "reinstall": "yarn clean && yarn install && yarn pod-install"
  },
  "dependencies": {
    "i18n-js": "3.3.0",
    "lodash.memoize": "4.1.2",
    "react": "16.8.6",
    "react-native": "0.60.5",
    "react-native-fs": "2.14.1",
    "react-native-localize": "file:../"
  },
  "devDependencies": {
    "@babel/core": "7.6.0",
    "@babel/runtime": "7.6.0",
    "metro-react-native-babel-preset": "0.56.0"
  }
}
